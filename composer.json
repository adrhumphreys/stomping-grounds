{
    "name": "silverstripe/recipe-cms",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.1 || ^8",
        "dnadesign/silverstripe-elemental": "3.1.3",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-core": "4.7.x-dev",
        "silverstripe/admin": "1.7.x-dev",
        "silverstripe/asset-admin": "1.7.x-dev",
        "silverstripe/campaign-admin": "1.7.x-dev",
        "silverstripe/versioned-admin": "1.7.x-dev",
        "silverstripe/cms": "4.7.x-dev",
        "silverstripe/errorpage": "1.7.x-dev",
        "silverstripe/reports": "4.7.x-dev",
        "silverstripe/siteconfig": "4.7.x-dev",
        "silverstripe/versioned": "1.7.x-dev",
        "silverstripe/graphql": "3.4.x-dev"
    },
    "require-dev": {
        "lekoala/silverstripe-debugbar": "dev-master",
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
